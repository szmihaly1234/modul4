<Window x:Class="modul4.Data"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:modul4"
        xmlns:vms="clr-namespace:modul4.ViewModel"
        mc:Ignorable="d"
        Title="Data" Height="450" Width="450">
    <Window.DataContext>
        <vms:MainWindowViewModel />
    </Window.DataContext>
    <Grid>
        <StackPanel>
            <Label Content="{Binding athlete.Name}" Padding="25"></Label>
            <Label Content="{Binding athlete.PersonalBest}" Padding="25"></Label>
            <Label Content="{Binding athlete.YearlyBest}" Padding="25"></Label>
            <Label Content="{Binding athlete.Club}" Padding="25"></Label>
            <Label Content="{Binding athlete.Value}" Padding="25"></Label>
        </StackPanel>
    </Grid>
</Window>
