<Window x:Class="modul4.DataWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:modul4"
        xmlns:vms="clr-namespace:modul4.ViewModel"
        mc:Ignorable="d"
        Title="Data" Height="450" Width="450">
    <Window.DataContext>
        <vms:DataViewModel />
    </Window.DataContext>
    <Grid>
        <StackPanel>
            <Label Content="{Binding actual.Name, UpdateSourceTrigger=Explicit}" Padding="25"></Label>
            <Label Content="{Binding actual.Club, UpdateSourceTrigger=Explicit}" Padding="25"></Label>
            <Label Content="{Binding actual.PersonalBest, UpdateSourceTrigger=Explicit}" Padding="25"></Label>
            <Label Content="{Binding actual.YearlyBest, UpdateSourceTrigger=Explicit}" Padding="25"></Label>
            <Label Content="{Binding actual.Value, UpdateSourceTrigger=Explicit}" Padding="25"></Label>
        </StackPanel>
    </Grid>
</Window>
